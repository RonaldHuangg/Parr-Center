{
    "name": "Dev Container",
    "dockerFile": "Dockerfile",
    "forwardPorts": [3000, 5001],
    "postCreateCommand": "cd backend && npm install && cd ../frontend && npm install",
    "customizations": {
      "vscode": {
        "settings": {
          "terminal.integrated.defaultProfile.linux": "bash"
        },
        "extensions": [
          "dbaeumer.vscode-eslint",
          "esbenp.prettier-vscode"
        ]
      }
    },
    "remoteUser": "node"
  }
  
  